{
    "name": "mugennsou/laravel-octane-extension-amphp",
    "description": "",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "authors": [
        {
            "name": "MA JIAN",
            "email": "mj501738931@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mugennsou\\LaravelOctaneExtension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mugennsou\\LaravelOctaneExtension\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mugennsou\\LaravelOctaneExtension\\OctaneExtensionAmphpServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "ext-posix": "*",
        "amphp/http-server": "v3.x-dev as 3",
        "amphp/http-server-form-parser": "v2.x-dev as 2",
        "amphp/http-server-static-content": "v2.x-dev as 2",
        "amphp/log": "v2.x-dev as 2",
        "mugennsou/laravel-octane-extension": "dev-master",
        "spatie/laravel-package-tools": "^1.10"
    },
    "require-dev": {
        "amphp/phpunit-util": "v3.x-dev as 3",
        "mockery/mockery": "^1.5",
        "mugennsou/php-cs-fixer-config": "^0.0.1",
        "orchestra/testbench": "^6.16 | ^7.0",
        "pestphp/pest": "^1.21",
        "phpstan/phpstan": "^1.8"
    }
}
